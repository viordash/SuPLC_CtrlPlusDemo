    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Ico2Ccode/bin/Debug/linux-x64/Ico2Ccode.dll",
            "args": [
                "${workspaceFolder}/../../Resources/Icons/button.ico",
                "${workspaceFolder}/Ico2Ccode/bin/Debug/linux-x64/button.h",
                "2"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "integratedTerminal"
        }
    ]
}